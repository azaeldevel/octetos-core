{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Octetos Core TDD",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-lcunit",
                "-lconfig++",
                "-std=c++20",
                "-Wall",
                "-pedantic",
                "-D", "ARCH_LINUX",
                "-D", "OCTETOS_SERVER",
                "-D", "OCTETOS_CORE_TDD=3",
                "-D", "OCTETOS_CORE_V3_TDD",
                "-D", "OCTETOS_CORE_VERSION_MAJOR=3",
                "-D", "OCTETOS_CORE_VERSION_MINOR=0",
                "-D", "OCTETOS_CORE_VERSION_PATCH=0",
                "${file}",
                "${workspaceFolder}/tests/v3.cc",
                "${workspaceFolder}/3/Buffer.cc",
                "${workspaceFolder}/3/Configuration.cc",
                "${workspaceFolder}/3/Exception.hh",
                "${workspaceFolder}/3/Lexer-C.hh",
                "${workspaceFolder}/3/Lexer.hh",
                "${workspaceFolder}/3/array.hh",
                "${workspaceFolder}/3/entity.hh",
                "${workspaceFolder}/3/numbers.hh",
                "${workspaceFolder}/3/platform.cc",
                "${workspaceFolder}/3/string.cc",
                "${workspaceFolder}/3/table.hh",
                "${workspaceFolder}/3/tree.cc",
                "${workspaceFolder}/3/version.cc",
                "-o",
                "${workspaceFolder}/bin/Debug/core-TDD"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "Developing Nodes",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-std=c++20",
                "-Wall",
                "-pedantic",
                "-D", "ARCH_LINUX",
                "-D", "OCTETOS_SERVER",
                "-D", "OCTETOS_CORE_TDD=3",
                "-D", "OCTETOS_CORE_V3_TDD",
                "-D", "OCTETOS_CORE_VERSION_MAJOR=3",
                "-D", "OCTETOS_CORE_VERSION_MINOR=0",
                "-D", "OCTETOS_CORE_VERSION_PATCH=0",
                "${file}",
                "${workspaceFolder}/3/nodes.hh",
                "-o",
                "${workspaceFolder}/bin/Debug/nodes-develop"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}